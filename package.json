        "@shelf/jest-mongodb": "^1.3.4",
        "autoprefixer": "^10.3.5",
        "babel-loader": "^8.2.2",
        "chai": "^4.3.3",
        "css-loader": "^5.2.7",
        "dotenv-webpack": "^6.0.4",
